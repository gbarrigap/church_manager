<h1>Baptisms</h1>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Priest</th>
      <th>Baptised</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @baptisms.each do |baptism| %>
    <tr>
      <td><%= baptism.date %></td>
      <td><%= baptism.priest.display_name %></td>
      <td><%= baptism.baptised.display_name %></td>
      <td><%= fa_icon 'eye' %></td>
    </tr>
    <% end %>
  </tbody>
</table>
